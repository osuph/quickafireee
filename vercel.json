{
    "cleanUrls": true,
    "trailingSlash": true,
    "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type",
        "Cache-Control": "public, max-age=25565, stale-while-revalidate"
    },
    "rewrites": [
        {
            "source": "/webserver_api.py",
            "destination": "/"
        }
    ]
}